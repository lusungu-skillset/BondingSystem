{
  "monorepo": true,
  "root": "apps/main",
  "sourceRoot": "apps/main/src",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/main/tsconfig.app.json"
  },
  "projects": {
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "bank-service": {
      "type": "application",
      "root": "apps/bank-service",
      "entryFile": "main",
      "sourceRoot": "apps/bank-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/bank-service/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "loan-service": {
      "type": "application",
      "root": "apps/loan-service",
      "entryFile": "main",
      "sourceRoot": "apps/loan-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/loan-service/tsconfig.app.json"
      }
    },
    "main": {
      "type": "application",
      "root": "apps/main",
      "entryFile": "main",
      "sourceRoot": "apps/main/src",
      "compilerOptions": {
        "tsConfigPath": "apps/main/tsconfig.app.json"
      }
    },
    "parent-service": {
      "type": "application",
      "root": "apps/parent-service",
      "entryFile": "main",
      "sourceRoot": "apps/parent-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/parent-service/tsconfig.app.json"
      }
    },
    "personal-service": {
      "type": "application",
      "root": "apps/personal-service",
      "entryFile": "main",
      "sourceRoot": "apps/personal-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/personal-service/tsconfig.app.json"
      }
    }
  }
}